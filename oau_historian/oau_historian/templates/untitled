			

			function openGenerator() {
				window.open("/report/generar", "windows", "directories=no,menubar =no,status=no,toolbar=no,location=no,scrollbars=no,fullscreen=no")
			}

			'<input type='button' onclick='openGenerator()' value='Generar Reporte'>'





